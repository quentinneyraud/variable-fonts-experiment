html, body
  margin 0
  padding 0
  overflow hidden

@font-face
  font-family Compressa
  font-style normal
  font-weight 400
  src url('Compressa.ttf') format('truetype')

.project-section
  height 100vh
  --wdth: 10
  --wght: 100

h1
  font-family Compressa
  font-size 400px
  line-height 400px
  margin-top -20px
  font-variation-settings 'wght' var(--wght), 'wdth' var(--wdth)
